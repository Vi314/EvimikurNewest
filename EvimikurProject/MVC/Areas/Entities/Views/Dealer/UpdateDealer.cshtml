@model DealerDTO
@{
	Layout = "_DashboardLayout";
}

<div>
	<form method="post">
		<div class="input-group mb-3">
			<span class="input-group-text">Bayii Adı</span>
			<input type="text" class="form-control" asp-for="Name"/>
		</div>
		<div class="input-group mb-3">
			<span class="input-group-text">Bayii Adres</span>
			<textarea type="text" class="form-control" asp-for="FullAddress"></textarea> 
		</div>
		<button class="btn btn-outline-success" type="submit">Kaydet</button>
	</form>
</div>
